					<sharing>PublishSubscribe</sharing>
					<transportation>HLAreliable</transportation>
					<order>Receive</order>
					<semantics>Identification number of Client</semantics>
				</attribute>
				<attribute>
					<name>distanceToFirm</name>
					<dataType>HLAinteger32BE</dataType>
